{
  "name": "@heath/fmp-api-client",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript client for the Financial Modeling Prep API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "docs:generate": "typedoc",
    "prepublishOnly": "npm run build && npm run test",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": [
    "financial",
    "modeling",
    "prep",
    "fmp",
    "api",
    "stock",
    "market",
    "typescript",
    "financial-data",
    "investment",
    "analysis"
  ],
  "author": "Heath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fmp-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/fmp-api-client/issues"
  },
  "homepage": "https://github.com/your-username/fmp-api-client#readme",
  "dependencies": {
    "axios": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.8",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.1.4",
    "tsx": "^4.15.7",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}